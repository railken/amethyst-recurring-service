{
    "name": "railken/amethyst-recurring-service",
    "keywords": ["laravel", "amethyst", "amethyst-recurring-service", "recurring-service"],
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "railken/amethyst-common": "0.1.*",
        "railken/amethyst-tax": "0.1.*",
        "railken/amethyst-catalogue": "0.1.*",
        "railken/amethyst-address": "0.1.*",
        "league/iso3166": "^2.1",
        "dannyvankooten/vat.php": "^1.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.10",
        "phpunit/phpunit": "~7.0",
        "orchestra/testbench": "3.6.*",
        "friendsofphp/php-cs-fixer": "^2.8.1"
    },
    "autoload": {
        "psr-4" : {
            "Railken\\Amethyst\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Railken\\Amethyst\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Railken\\Amethyst\\Providers\\RecurringServiceServiceProvider"
            ]
        }
    }
}
